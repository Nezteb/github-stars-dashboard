{
    "compilerOptions": {
        "target": "es2017",
        "lib": [
            "esnext"
        ],
        "allowJs": false,
        "skipLibCheck": false,
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "forceConsistentCasingInFileNames": true,
        "module": "commonjs",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "sourceMap": true,
        "inlineSources": true
    },
    "include": [
        "./get_stars_json.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}